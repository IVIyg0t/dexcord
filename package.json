    "lokijs": "^1.5.12",
    "lowdb": "^3.0.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.19.0",
    "sqlite3": "^5.0.6"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.182",
    "@types/lokijs": "^1.5.7",
    "@types/node": "^17.0.23",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.2",
    "ts-node": "^10.7.0",
